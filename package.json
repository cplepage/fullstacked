  "bin": "./cli.js",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/react": "^17.0.41",
    "esbuild": "^0.14.27",
    "express": "^4.17.3",
    "glob": "^7.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
