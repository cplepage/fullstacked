{
  "name": "fullstacked",
  "title": "FullStacked",
  "version": "0.4.0-alpha.9",
  "description": "A full stack web app tool and development kit",
  "types": "index.d.ts",
  "scripts": {
    "preinstall": "npm config set puppeteer_product=chrome",
    "build": "npx esbuild build-cli.ts --outfile=build-cli.js --format=cjs && node build-cli",
    "pretest": "npm run build",
    "test": "node cli test --headless",
    "precoverage": "npm run build",
    "coverage": "node cli test --headless --coverage",
    "prepublishOnly": "npm run build && tsc -p tsconfig.emitDeclaration.json --emitDeclarationOnly"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CPLepage/fullstacked.git"
  },
  "keywords": [
    "webapp",
    "typescript",
    "fullstack",
    "tool"
  ],
  "author": "CP Lepage",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CPLepage/fullstacked/issues"
  },
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/fullstacked"
  },
  "homepage": "https://fullstacked.org",
  "bin": {
    "fullstacked": "cli.js"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.0",
    "@types/react": "^18.0.8",
    "axios": "^0.26.1",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.27",
    "express": "^4.17.3",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "morgan": "^1.10.0",
    "nyc": "^15.1.0",
    "puppeteer": "^13.7.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "ssh2-sftp-client": "^7.2.3",
    "v8-to-istanbul": "^8.1.1",
    "ws": "^8.5.0",
    "yaml": "^2.0.1"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/ws": "^8.5.3",
    "mongodb": "^4.6.0",
    "typescript": "^4.6.3"
  }
}
