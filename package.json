{
  "name": "fullstacked",
  "title": "FullStacked",
  "version": "0.0.22-beta2",
  "description": "A full stack web app build tool and development kit",
  "scripts": {
    "build": "esbuild build-cli.ts --outfile=build-cli.js --format=cjs && node build-cli",
    "pretest": "npm run build",
    "test": "mocha \"!(scripts)/**/test.ts\"",
    "coverage": "npx nyc --reporter text-summary --reporter html npm test",
    "prepublishOnly": "npm run build && tsc -p tsconfig.emitDeclaration.json --emitDeclarationOnly"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CPLepage/fullstacked.git"
  },
  "keywords": [
    "webapp",
    "typescript",
    "fullstack",
    "build",
    "tool"
  ],
  "author": "CP Lepage",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CPLepage/fullstacked/issues"
  },
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/fullstacked"
  },
  "homepage": "https://fullstacked.org",
  "bin": "./cli.js",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.0",
    "@types/react": "^17.0.41",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.27",
    "express": "^4.17.3",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "morgan": "^1.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@types/glob": "^7.2.0",
    "axios": "^0.26.1",
    "puppeteer": "^13.5.1",
    "react-bootstrap": "^2.2.2",
    "react-bootstrap-typeahead": "^6.0.0-alpha.10",
    "react-router-dom": "^6.2.2",
    "ssh2-sftp-client": "^7.2.3",
    "typescript": "^4.6.3",
    "v8-to-istanbul": "^8.1.1"
  }
}
