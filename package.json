{
  "name": "fullstacked",
  "title": "FullStacked",
  "version": "0.10.0-1",
  "description": "A simple tool that takes care of all the redundant tasks in TypeScript web app development.",
  "types": "index.d.ts",
  "scripts": {
    "build": "npm run clean && npx esbuild build.ts --outfile=build.js --format=esm && node build",
    "clean": "npx esbuild clean.ts --outfile=clean.js --format=esm && node clean",
    "prepack": "npm run build",
    "pretest": "npm run build",
    "test": "node test"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cplepage/fullstacked.git"
  },
  "keywords": [
    "webapp",
    "typescript",
    "fullstack",
    "tool"
  ],
  "author": "CP Lepage",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cplepage/fullstacked/issues"
  },
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/fullstacked"
  },
  "homepage": "https://fullstacked.org",
  "bin": {
    "fullstacked": "cli.js"
  },
  "workspaces": [
    "commands/backup",
    "commands/build",
    "commands/deploy",
    "commands/run",
    "commands/test",
    "commands/watch",
    "create"
  ],
  "devDependencies": {
    "@types/node": "^18.14.2",
    "esbuild": "^0.17.10"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "cli-table3": "^0.6.3",
    "docker-names": "^1.2.1",
    "mime": "^3.0.0",
    "ws": "^8.12.1"
  },
  "exports": {
    "./commands/info": "./commands/info/index.js",
    "./*": "./*.js"
  }
}
