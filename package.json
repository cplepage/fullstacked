{
  "name": "fullstacked",
  "title": "FullStacked",
  "version": "0.8.3-alpha2",
  "description": "A full stack web app tool and development kit",
  "types": "index.d.ts",
  "scripts": {
    "preinstall": "npm config set puppeteer_product=chrome",
    "build": "npx esbuild build.ts --outfile=build.js --format=cjs && node build",
    "pretest": "npm run build",
    "test": "node cli test --headless",
    "coverage": "npm test -- --coverage",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CPLepage/fullstacked.git"
  },
  "keywords": [
    "webapp",
    "typescript",
    "fullstack",
    "tool"
  ],
  "author": "CP Lepage",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CPLepage/fullstacked/issues"
  },
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/fullstacked"
  },
  "homepage": "https://fullstacked.org",
  "bin": {
    "fullstacked": "cli.js"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.27",
    "glob": "^7.2.0",
    "mime-types": "^2.1.35",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "progress-stream": "^2.0.0",
    "puppeteer": "^13.7.0",
    "ssh2-sftp-client": "^9.0.4",
    "v8-to-istanbul": "^8.1.1",
    "ws": "^8.5.0",
    "yaml": "^2.0.1"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/node": "^18.8.5",
