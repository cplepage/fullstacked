{
  "name": "fullstacked",
  "title": "FullStacked",
  "version": "0.1.6",
  "description": "A full stack web app tool and development kit",
  "scripts": {
    "postinstall": "node postinstall",
    "build": "esbuild build-cli.ts --outfile=build-cli.js --format=cjs && node build-cli",
    "pretest": "npm run build",
    "test": "node cli test --headless",
    "precoverage": "npm run build",
    "coverage": "node cli test --headless --coverage",
    "prepublishOnly": "npm run build && tsc -p tsconfig.emitDeclaration.json --emitDeclarationOnly",
    "website": "node cli watch --src=website"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CPLepage/fullstacked.git"
  },
  "keywords": [
    "webapp",
    "typescript",
    "fullstack",
    "tool"
  ],
  "author": "CP Lepage",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CPLepage/fullstacked/issues"
  },
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/fullstacked"
  },
  "homepage": "https://fullstacked.org",
  "bin": {
    "fullstacked": "cli.js"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.0",
    "@types/react": "^18.0.8",
    "axios": "^0.26.1",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.27",
    "express": "^4.17.3",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "morgan": "^1.10.0",
    "puppeteer": "^13.5.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "ssh2-sftp-client": "^7.2.3",
    "v8-to-istanbul": "^8.1.1",
    "ws": "^8.5.0",
    "yaml": "^2.0.1"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@types/glob": "^7.2.0",
    "@types/ws": "^8.5.3",
    "react-bootstrap": "^2.2.2",
    "react-bootstrap-typeahead": "^6.0.0-alpha.10",
    "react-router-dom": "^6.2.2",
    "typescript": "^4.6.3"
  }
}
