{
  "name": "fullstacked",
  "description": "A Ubiquitous Workspace for Developers",
  "version": "0.12.1",
  "author": "cplepage",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fullstackedorg/workspace.git"
  },
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/fullstacked"
  },
  "homepage": "https://fullstacked.org",
  "dependencies": {
    "node-pty": "^0.10.1"
  },
  "bin": {
    "fullstacked": "index.js"
  },
  "scripts": {
    "typecheck": "tsc --noEmit --skipLibCheck",
    "start": "fsc watch -c ./src/main/client/index.tsx -s src/main/server/index.ts",
    "cp": "node src/main/cp",
    "build": "node build && npm run cp",
    "predocker": "npm run build",
    "docker": "docker build . -f docker/Dockerfile -t fullstackedorg/workspace:test",
    "prepack": "npm run build",
    "preelectron": "npm run build",
    "electron": "electron electron/index.mjs"
  },
  "type": "module",
  "devDependencies": {
    "@codemirror/autocomplete": "^6.9.0",
    "@codemirror/lang-css": "^6.2.1",
    "@codemirror/lang-html": "^6.4.5",
    "@codemirror/lang-javascript": "^6.1.9",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-markdown": "^6.2.0",
    "@codemirror/lang-sass": "^6.0.2",
    "@codemirror/legacy-modes": "^6.3.3",
    "@codemirror/lint": "^6.4.0",
    "@codemirror/merge": "^6.1.3",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@codemirror/view": "^6.16.0",
    "@fullstacked/build": "^0.10.12",
    "@fullstacked/cli": "^0.10.14",
    "@fullstacked/share": "^0.10.9",
    "@fullstacked/sync": "^0.10.1",
    "@fullstacked/watch": "^0.10.24",
    "@fullstacked/webapp": "^0.10.18",
    "@types/cookie": "^0.5.1",
    "@types/http-proxy": "^1.17.11",
    "@types/js-cookie": "^3.0.3",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@types/ws": "^8.5.5",
    "codemirror": "^6.0.1",
    "cookie": "^0.5.0",
    "decompress": "^4.2.1",
    "dotenv": "^16.3.1",
    "electron": "^28.0.0",
    "eslint-linter-browserify": "^8.46.0",
    "fast-querystring": "^1.1.2",
    "http-proxy": "^1.18.1",
    "ignore": "^5.2.4",
    "js-cookie": "^3.0.5",
    "open": "^9.1.0",
    "pretty-bytes": "^6.1.1",
