{
  "name": "fullstacked",
  "title": "FullStacked",
  "version": "0.9.0-alpha2",
  "description": "A full stack web app tool and development kit",
  "types": "index.d.ts",
  "scripts": {
    "build:gui": "node cli build --src=gui --out=gui",
    "build": "npm run cleanup && npx esbuild build.ts --outfile=build.js --format=esm && node build && npm run build:gui",
    "cleanup": "npx esbuild cleanup.ts --outfile=cleanup.js --format=esm && node cleanup",
    "prestart": "npm run build",
    "start": "node cli watch --src=gui",
    "pretest": "npm run build && node utils/testsDockerImages",
    "test": "node cli test --headless",
    "coverage": "npm test -- --coverage",
    "prepare": "npm run build && npx tsc"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CPLepage/fullstacked.git"
  },
  "keywords": [
    "webapp",
    "typescript",
    "fullstack",
    "tool"
  ],
  "author": "CP Lepage",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CPLepage/fullstacked/issues"
  },
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/fullstacked"
  },
  "homepage": "https://fullstacked.org",
  "bin": {
    "fullstacked": "cli.mjs"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "c8": "^7.12.0",
    "dockerode": "^3.3.4",
    "dockerode-compose": "^1.3.2",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.27",
    "glob": "^7.2.0",
    "js-yaml": "^4.1.0",
    "mime": "^3.0.0",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "open": "^8.4.0",
    "parse5": "^7.1.2",
    "progress-stream": "^2.0.0",
    "puppeteer": "^19.4.0",
    "ssh2-sftp-client": "^9.0.4",
    "v8-to-istanbul": "^8.1.1",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@tabler/core": "^1.0.0-beta16",
    "@types/dockerode": "^3.3.14",
    "@types/express": "^4.17.14",
    "@types/glob": "^7.2.0",
    "@types/http-proxy": "^1.17.9",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.11.16",
    "@types/react": "^18.0.26",
    "@types/semver": "^7.3.13",
    "@types/ssh2-sftp-client": "^9.0.0",
    "@types/ws": "^8.5.3",
    "cookie": "^0.5.0",
    "express": "^4.18.2",
    "http-proxy": "^1.18.1",
    "js-cookie": "^3.0.1",
    "lowdb": "^5.0.5",
    "mongodb": "^4.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.4"
  }
}
